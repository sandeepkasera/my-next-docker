{
  "family": "my-next-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::545746263033:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "my-next-container",
      "image": "my-next-docker",
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
